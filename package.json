{
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:fast": "NODE_OPTIONS='--max-old-space-size=4096' next dev",
    "build": "PUPPETEER_SKIP_CHROMIUM_DOWNLOAD=true next build",
    "build:analyze": "ANALYZE=true next build",
    "build:production": "npm run cache:clear && npm run build && npm run cache:invalidate",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "performance:check": "npm run build:analyze && echo 'Bundle analysis completed. Check the generated files for optimization opportunities.'",
    "cache:invalidate": "node scripts/production-cache-invalidation.js",
    "cache:clear": "rm -rf .next && npm cache clean --force",
    "cache:clear-all": "npm run cache:clear && npm run cache:invalidate",
    "cache:purge": "curl -X POST \"$NEXT_PUBLIC_SITE_URL/api/webhooks/cache-purge\" -H \"Content-Type: application/json\" -H \"Authorization: Bearer $WEBHOOK_SECRET\" -d '{\"action\":\"purge_all\",\"source\":\"manual\"}'",
    "cache:warm": "node -e \"require('./lib/production-cache-advanced.js').CacheWarmer.warmCache()\"",
    "deploy": "npm run build:production",
    "deploy:immediate": "npm run cache:clear-all && npm run build && npm run cache:invalidate",
    "deploy:production": "chmod +x scripts/deploy-with-cache.sh && ./scripts/deploy-with-cache.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "security:check": "./scripts/security-check.sh",
    "security:audit": "npm audit --audit-level=moderate",
    "security:test": "npm run test -- --testPathPattern=security",
    "test:local": "./scripts/test-ci-local.sh all",
    "test:local:security": "./scripts/test-ci-local.sh security",
    "test:local:build": "./scripts/test-ci-local.sh build",
    "test:local:vercel": "./scripts/test-ci-local.sh vercel",
    "vercel:setup": "./scripts/setup-vercel-config.sh",
    "lighthouse": "lhci autorun",
    "lighthouse:local": "lhci autorun --config=lighthouserc.js",
    "load-test": "./scripts/run-load-tests.sh both",
    "load-test:k6": "./scripts/run-load-tests.sh k6",
    "load-test:artillery": "./scripts/run-load-tests.sh artillery",
    "load-test:local": "./scripts/run-load-tests.sh both http://localhost:3000",
    "load-test:production": "./scripts/run-load-tests.sh both https://codeunia.com",
    "performance-test": "node scripts/simple-performance-test.js",
    "lighthouse:install": "npm install -g @lhci/cli@0.12.x",
    "lighthouse:test": "./scripts/lighthouse-test.sh local",
